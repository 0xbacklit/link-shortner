generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          BigInt   @id @default(autoincrement())
  shortCode   String   @unique
  longUrl     String
  expiresAt   DateTime?
  createdAt   DateTime @default(now())

  @@index([expiresAt])
}
